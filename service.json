{
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
        "labels": {
            "app": "authenticator",
            "version": "1.0.1"
        },
        "name": "authenticator",
        "namespace": "mac-services"
    },
    "spec": {
        "clusterIP": "192.168.0.0",
        "ports": [
            {
                "nodePort": 31797,
                "port": 80,
                "protocol": "TCP",
                "targetPort": 4242
            }
        ],
        "selector": {
            "app": "authenticator",
            "version": "1.0.1"
        },
        "sessionAffinity": "None",
        "type": "LoadBalancer"
    }
}
